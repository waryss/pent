{
  "name": "pent",
  "version": "0.0.1",
  "description": "Pent needs managemet api",
  "main": "dist",
  "author": "waryss",
  "license": "ISC",
  "engines": {
    "node": ">=7.7.3",
    "npm": ">=4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "elasticsearch": "^13.3.1",
    "express": "^4.15.3",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "nodemailer": "^4.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  }
}
